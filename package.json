{
  "name": "craftea",
  "version": "0.1.0",
  "description": "Web-based 3D structure design tool",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "postinstall": "cd node_modules/.pnpm/better-sqlite3@*/node_modules/better-sqlite3 2>/dev/null && npm run build-release || true"
  },
  "keywords": [
    "3d",
    "cad",
    "design",
    "structure"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.18.0",
    "better-sqlite3": "^11.10.0",
    "drizzle-orm": "^0.44.7",
    "next": "^14.2.33",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "three": "^0.181.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@types/three": "^0.181.0",
    "autoprefixer": "^10.4.0",
    "drizzle-kit": "^0.31.7",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.3",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.9.3"
  }
}
